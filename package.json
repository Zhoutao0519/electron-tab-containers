{
  "name": "electron-tab-containers",
  "version": "1.0.0",
  "description": "Electron 多标签页多容器",
  "author": "zhoutao",
  "license": "MIT",
  "private": false,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "render:dev": "vite",
    "render:build": "vite build",
    "dev": "concurrently \"npm run render:dev\" \"npm run start\"",
    "start": "electron-forge start",
    "package": "npm run build && npm run render:build && electron-forge package",
    "make": "npm run build && npm run render:build && electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^20.8.10",
    "concurrently": "^9.2.1",
    "electron": "^27.0.3",
    "png-to-ico": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
